{
    "name": "amjadiqbal/filament-url-image-uploader",
    "type": "library",
    "description": "A powerful Filament PHP form component that enables seamless image uploads from URLs. Features include image validation, preview functionality, and easy integration with Laravel storage. Perfect for remote image imports, content management, and e-commerce applications built with FilamentPHP.",
    "keywords": [
        "amjadiqbal",
        "laravel",
        "filament",
        "filament-url-image-uploader",
        "image-upload",
        "url-upload",
        "filament-plugin",
        "remote-image",
        "image-import",
        "filament-component",
        "laravel-package",
        "filament-form"
    ],
    "homepage": "https://github.com/amjadiqbal/filament-url-image-uploader",
    "license": "MIT",
    "authors": [
        {
            "name": "Amjad Iqbal",
            "role": "Developer",
            "homepage": "https://www.upwork.com/freelancers/amjadkhatri"
        }
    ],
    "support": {
        "docs": "ttps://devodocs.com/laravel/filament-url-image-uploader",
        "issues": "https://github.com/amjadiqbal/filament-url-image-uploader/issues",
        "source": "https://github.com/amjadiqbal/filament-url-image-uploader",
        "discord": "https://discord.com/channels/1352854772859932702/1352854916690874388"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/amjadiqbal/"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "AmjadIqbal\\FilamentUrlImageUploader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AmjadIqbal\\FilamentUrlImageUploader\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "AmjadIqbal\\FilamentUrlImageUploader\\FilamentUrlImageUploaderServiceProvider"
            ]
        }
    }
}